{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "allowJs": true,
    "types": [
      "astro/client",
      "vite/client"
    ],
    "plugins": [{"name": "@astrojs/ts-plugin"},],
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
    },
    "strictNullChecks": true,
    "jsx": "preserve"
  }
}