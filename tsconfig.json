{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "allowJs": true,
    "types": ["astro/client","vite/client", "vitest"],
    "plugins": [{"name": "@astrojs/ts-plugin"}],
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "paths": {"@/*": ["./src/*"]},
    "strictNullChecks": true,
  }
}