{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "https://localhost:4321",
  "frontMatter.content": "/src/assets",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.data.files": [
    {
      "id": "skills",
      "title": "Skills",
      "file": "[[workspace]]/src/content/cv/Skills.json",
      "fileType": "json",
      "labelField": "name",
      "singleEntry": false,
      "schema": {
        "title": "Skills",
        "type": "object",
        "required": [
          "name",
          "level",
          "category"
        ],
        "properties": {
          "name": {
            "title": "Name",
            "type": "string"
          },
          "level": {
            "title": "Level",
            "type": "string",
            "enum": [
              "⭐",
              "⭐⭐",
              "⭐⭐⭐",
              "⭐⭐⭐⭐",
              "⭐⭐⭐⭐⭐"
            ]
          },
          "category": {
            "title": "Category",
            "type": "string",
            "enum": [
              "Languages",
              "Frameworks",
              "Libraries",
              "Tools",
              "Databases",
              "Platforms",
              "Other",
              "hardware"
            ]
          }
        }
      }
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog",
      "contentTypes": [
        "blog"
      ]
    },
    {
      "title": "projecten",
      "path": "[[workspace]]/src/content/projecten",
      "contentTypes": [
        "projecten"
      ]
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": "'blog'",
      "filePrefix": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "datetime"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "pubDate",
          "name": "pubDate",
          "type": "datetime"
        },
        {
          "title": "updatedDate",
          "name": "updatedDate",
          "type": "datetime"
        },
        {
          "title": "badge",
          "name": "badge",
          "type": "string"
        },
        {
          "title": "heroImage",
          "name": "heroImage",
          "type": "image"
        },
        {
          "title": "slug",
          "name": "slug",
          "type": "datetime"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.website.host": "https://robstolte.nl"
}